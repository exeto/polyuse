{
  "name": "polyuse",
  "version": "0.0.0",
  "description": "A modular library that provides building blocks for creating your own framework",
  "license": "MIT",
  "homepage": "https://polyuse.dev",
  "author": "Timofey Dergachev <hi@exeto.dev> (https://exeto.dev)",
  "repository": "exeto/polyuse",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./ioc": "./dist/ioc/index.js"
  },
  "sideEffects": false,
  "keywords": [
    "di",
    "ioc"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vitest",
    "build": "rm -rf dist && tsc",
    "lint": "eslint && prettier --check .",
    "lint:fix": "eslint --fix && prettier --write .",
    "test": "vitest run",
    "release": "pnpm run build && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.2"
  }
}
